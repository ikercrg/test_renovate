{
  "name": "web-frontend",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy-local-backend.conf.js",
    "start:remote-dev": "ng serve --proxy-config proxy-remote-dev-backend.conf.js",
    "build": "ng build",
    "buildProd": "ng build --prod",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:clear-cache": "jest --clearCache",
    "lint": "ng lint",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:dev": "cypress run --config video=false",
    "pretest": "ng lint",
    "oss-clearing": "oss-clearing"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "modulePathIgnorePatterns": [
      "./e2e",
      "./jestGlobalMocks.ts"
    ],
    "coveragePathIgnorePatterns": [
      "./jestGlobalMocks.ts",
      "./src/app/generated",
      "./src/app/logger/console-log-appender.ts"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "10.2.5",
    "@angular/core": "10.2.5",
    "rxjs": "6.6.7"
  },
  "devDependencies": {
    "@angular/cli": "10.2.3",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0"
  }
}
